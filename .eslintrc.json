{
  "env": {
    "es2021": true,
    "node": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "import/no-default-export": "error",
    "import/extensions": ["error", "always"]
  }
}
